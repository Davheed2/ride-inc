{
	"name": "millenia-trades-backend",
	"version": "1.0.0",
	"description": "Expressjs project generated using startease CLI tool",
	"main": "server.js",
	"scripts": {
		"start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/server.js",
		"start-prod": "npm run migrate && node -r module-alias/register build/server.js",
		"migrate": "npx ts-node -r dotenv/config node_modules/.bin/knex migrate:latest --knexfile build/knexfile.js",
		"build": "tsc",
		"migrate-dev": "npx ts-node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:latest --knexfile src/knexfile.ts",
		"make-migration": "npx ts-node -r dotenv/config ./node_modules/knex/bin/cli.js migrate:make --knexfile src/knexfile.ts",
		"dev": "npm run migrate-dev && nodemon",
		"watch": "tsc --watch & nodemon build/server.js",
		"lint": "npx eslint src/",
		"format": "npx prettier --write .",
		"check-types": "tsc --noEmit --pretty --skipLibCheck --incremental --project tsconfig.json --strict"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.782.0",
