		"@types/multer": "^1.4.11",
		"axios": "^1.12.2",
		"bcryptjs": "^3.0.2",
		"bullmq": "^5.46.1",
		"compression": "^1.7.4",
		"connect-timeout": "^1.9.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^5.0.1",
		"express-mongo-sanitize": "^2.2.0",
		"express-rate-limit": "^7.3.1",
		"glob": "^11.0.3",
		"helmet": "^8.0.0",
		"helmet-csp": "^4.0.0",
		"hi-base32": "^0.5.1",
		"hpp": "^0.2.3",
		"ioredis": "^5.6.0",
		"jsonwebtoken": "^9.0.2",
